local song = game:GetService("InsertService"):LoadAsset(2777740)

song:Play()

game:GetService("Chat"):Chat(game.Players.LocalPlayer, "L", Enum.ChatColor.Red)

local image = "https://www.roblox.com/asset?id=6403436082"
local imageAsset = game:GetService("InsertService"):LoadAsset(image)

local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(1, 0, 1, 0)
imageLabel.Image = imageAsset
imageLabel.Parent = game.Players.LocalPlayer.PlayerGui
